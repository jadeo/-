/* my_news.wxss */
.container{
  width:100%;
}
.my_news{
  padding:30rpx 24rpx 30rpx 30rpx;
  background: #fff;
  height:106rpx;
  margin-top:20rpx;
  width:700rpx;
}
.my_news .my_news_detail{
  display:flex;
}
.my_news .my_news_detail image{
  width:60rpx;
  height:60rpx;
  margin-right:24rpx;
  border-radius: 50%;

}
.name{
  display:inline-block;
  flex-grow:4;
  font-size:32rpx;
  color:#333334;
  margin-top:14rpx;
}
.time{
  font-size:24rpx;
  color:#999999;
  margin-top:14rpx;
  margin-right:15rpx;
}
.do_des{
  font-size:28rpx;
  color:#333333;
  margin-left:84rpx;
}
.do_des_comment{
  color:#333333;
}
.do_des_detail{
  color:#666666;
  text-overflow: hidden;
}


/*没有消息*/
